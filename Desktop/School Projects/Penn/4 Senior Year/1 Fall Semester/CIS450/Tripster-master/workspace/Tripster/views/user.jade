// Jade file to specify output of an actors list
// Inherits from layout
extend layout
block content
  p
    | #{title}: 
  // Create a table with one row per result
  table
        tr
          td.user_name=user_info[0].FIRST_NAME + " " + user_info[0].LAST_NAME
        tr
          td.user_aff=user_info[0].AFFILIATION
        tr
          td.user_email=user_info[0].EMAIL
    
      